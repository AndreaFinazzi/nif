# - Config file for the $LuminLib package
# It defines the following variables
#  $LuminLib_INCLUDE_DIRS - include directories for $LuminLib
#  $LuminLib_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(LuminLib_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LuminLib_INCLUDE_DIRS "@CONFIG_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET LuminLib AND NOT LuminLib_BINARY_DIR)
  include("${LuminLib_CMAKE_DIR}/LuminLibTargets.cmake")
endif()
 
# These are IMPORTED targets created by LuminLibTargets.cmake
set(LuminLib_LIBRARIES "@EXPORT_TARGETS@")